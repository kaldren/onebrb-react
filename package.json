{
  "name": "onebrb",
  "version": "1.0.0",
  "description": "Onebrb app.",
  "main": "backend/dist/server.js",
  "type": "module",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "start:ts": "tsc -w",
    "start:js": "nodemon backend/dist/server.js",
    "client": "npm start --prefix frontend",
    "data:i": "node backend/seeder.js -i",
    "data:d": "node backend/seeder.js -d",
    "dev": "concurrently \"npm:start:*\" \"npm run client\"",
    "data:i2": "concurrently \"npm run start:ts\" \"npm run data:i\"",
    "data:d2": "concurrently \"npm run start:ts\" \"npm run data:d\"",
    "build": "npm build"
  },
  "author": "Kaloyan Drenski",
  "license": "MIT",
  "dependencies": {
    "@types/color": "^3.0.3",
    "@types/colors": "^1.2.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "mongoose": "^6.2.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.26",
    "colors": "^1.4.0",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.2"
  }
}
